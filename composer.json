{
    "name": "acredula/restful-data-mapper",
    "description": "A data mapper aimed at simplifying the Repository and DataMapper layers of a RESTful API",
    "keywords": [
        "acredula",
        "data",
        "mapper",
        "respository",
        "psr-7"
    ],
    "homepage": "https://github.com/acredula/restful-data-mapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Phil Bennett",
            "email": "philipobenito@gmail.com",
            "homepage": "http://www.philipobenito.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Acredula\\DataMapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acredula\\DataMapper\\Test\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1-dev"
        }
    }
}
